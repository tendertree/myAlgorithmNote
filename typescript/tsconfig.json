{
	"compilerOptions": {
		"baseUrl": "src/",
		"target": "es2016",
		"module": "esnext",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"moduleResolution": "node"
	}
},
"include": [
	"./src/**/*",
	"./__tests__/**/*"
],
"exclude": [
	"node_modules"
],
"ts-node": {
	"require": [
		"tsconfig-paths/register"
	]
}
}
